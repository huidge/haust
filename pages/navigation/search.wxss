.search {
    width: 96%;
    height: 80rpx;
    background-color: #f5f5f5;
    border-radius: 15px;
    margin: 20rpx 2%;
    display: flex;
}

.search-icon {
    margin: auto 20rpx;
}

.search-form {
    margin: auto 15rpx;
    width: 100%;
}

.placesItem {
    display: flex;
    width: 100%;
    height: 160rpx;
    border-bottom: solid 1px #eee;
}

.placesItemView {
    display: flex;
    margin: auto;
}

.navPlacesToProfile {
    display: flex;
    align-self: center;
    width: 600rpx;
}

.nodata {
    text-align: center;
    font-size: 35rpx;
    color: #777;
    margin: 45rpx;
}

.search-type {
    display: flex;
    height: 60rpx;
    width: 100%;
    color: #333;
    justify-content: center;
    align-items: center;
    border-radius: 30rpx;
    background-color: #f2f9ff;
    margin-top: 30rpx;
    margin-bottom: 10rpx;
}

.other-type {
    margin-top: 100rpx;
}

.itemcontent {
    width: 400rpx;
    margin-left: 20rpx;
}

.itemName {
    font-size: 34rpx;
}

.locationText {
    color: #aaa;
    display: -webkit-box;
    font-size: 28rpx;
    line-height: 40rpx;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
}

.other-search {
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    text-overflow: ellipsis;
}

.navPlacesToPolyline {
    display: flex;
    justify-content: center;
    align-self: center;
}

.itemImg {
    height: 98.88rpx;
    width: 160rpx;
    margin: auto;
}

.itemPolyline {
    width: 80rpx;
    height: 80rpx;
    margin: auto;
}

.btn-feedback {
    background-color: white;
    color: #2196F3;
}

button::after {
    display: none;
}